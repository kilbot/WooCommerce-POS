{
  "name": "woocommerce-pos",
  "description": "A simple front-end for taking WooCommerce orders at the Point of Sale.",
  "version": "0.5.0-beta",
  "scripts": {
    "build": "npm install && node_modules/.bin/grunt build --force && npm run start",
    "start": "docker-compose -f .docker/docker.yml build && docker-compose -f .docker/docker.yml up -d",
    "start:php54": "docker-compose -f .docker/docker-php54.yml build && docker-compose -f .docker/docker-php54.yml up -d",
    "start:php55": "docker-compose -f .docker/docker-php55.yml build && docker-compose -f .docker/docker-php55.yml up -d",
    "start:php56": "docker-compose -f .docker/docker-php56.yml build && docker-compose -f .docker/docker-php56.yml up -d",
    "start:php70": "docker-compose -f .docker/docker-php70.yml build && docker-compose -f .docker/docker-php70.yml up -d",
    "stop": "docker-compose -f .docker/docker.yml kill",
    "dev": "npm run build && node_modules/.bin/grunt dev --force",
    "deploy": "npm install && node_modules/.bin/grunt grunt deploy",
    "unit-test:js": "npm install && node_modules/.bin/grunt test",
    "unit-test:php54": "docker exec php54 phpunit -c wp-content/plugins/woocommerce-pos/tests/unit/php/phpunit.xml",
    "unit-test:php55": "docker exec php55 phpunit -c wp-content/plugins/woocommerce-pos/tests/unit/php/phpunit.xml",
    "unit-test:php56": "docker exec php56 phpunit -c wp-content/plugins/woocommerce-pos/tests/unit/php/phpunit.xml",
    "unit-test:php70": "docker exec php70 phpunit -c wp-content/plugins/woocommerce-pos/tests/unit/php/phpunit.xml",
    "teardown-docker": "npm run stop && docker rm $(docker ps -a -q) && rm -rf .data && rm -rf .html",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kilbot/WooCommerce-POS.git"
  },
  "keywords": [
    "wordpress",
    "woocommerce",
    "ecommerce",
    "point-of-sale",
    "backbone",
    "marionette"
  ],
  "author": "kilbot",
  "license": "GPL-3.0+",
  "homepage": "http://woopos.com.au",
  "bugs": {
    "url": "https://github.com/kilbot/WooCommerce-POS/issues"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "backbone": "^1.3.3",
    "backbone-deep-model": "kilbot/backbone-deep-model",
    "backbone-validation": "^0.11.5",
    "backbone.marionette": "^2.4.7",
    "backbone.radio": "^1.0.4",
    "backbone.stickit": "^0.9.2",
    "combokeys": "^2.4.6",
    "debug": "^2.2.0",
    "handlebars": "^4.0.5",
    "jquery": "^3.0.0",
    "jquery-color": "^1.0.0",
    "json-query": "kilbot/JSON-Query",
    "lodash": "^3.10.1",
    "moment": "^2.13.0",
    "node-polyglot": "^2.0.0",
    "normalize.css": "^4.2.0",
    "query-parser": "kilbot/Query-Parser",
    "select2": "^4.0.3",
    "tether": "^1.3.2",
    "tether-drop": "^1.4.2",
    "vex-js": "^2.3.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-backbone": "^0.9.4",
    "chai-jquery": "^2.0.0",
    "coveralls": "^2.11.9",
    "exports-loader": "^0.6.3",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compass": "^1.1.1",
    "grunt-contrib-compress": "^1.3.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-symlink": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-mocha-istanbul": "^5.0.1",
    "grunt-phpunit": "^0.3.6",
    "grunt-simple-mocha": "^0.4.1",
    "grunt-webpack": "^1.0.11",
    "grunt-wp-i18n": "^0.5.4",
    "imports-loader": "^0.6.5",
    "istanbul": "^0.4.4",
    "jsdom": "^9.3.0",
    "jshint-stylish": "^2.2.0",
    "load-grunt-tasks": "^3.5.0",
    "mocha": "^2.4.5",
    "node-localstorage": "^1.3.0",
    "proxyquire": "^1.7.10",
    "raw-loader": "^0.5.1",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
